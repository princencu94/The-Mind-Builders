dleSubmit = async (e) => {\r\n      e.preventDefault();\r\n      const data = new FormData()\r\n      data.append('firstname', assignResumeDetails.firstname);\r\n      data.append('lastname', assignResumeDetails.lastname);\r\n      data.append('email', assignResumeDetails.email);\r\n      data.append('streetaddress', assignResumeDetails.streetaddress);\r\n      data.append('city', assignResumeDetails.city);\r\n      data.append('region', assignResumeDetails.region);\r\n      data.append('postalcode', assignResumeDetails.postalcode);\r\n      data.append('previousjobtitle', assignResumeDetails.previousjobtitle);\r\n      data.append('jobstartdate', assignResumeDetails.jobstartdate);\r\n      data.append('jobenddate', assignResumeDetails.jobenddate);\r\n      data.append('about', assignResumeDetails.about);\r\n      data.append('file', selectedFile);\r\n\r\n      console.log(data);\r\n\r\n      try {\r\n          await fetch(\"http://localhost:5000/sendresume\", {\r\n              method: \"POST\",\r\n              headers: {\r\n              \"Content-type\": \"multipart/form-data\",\r\n              },\r\n              body: data ,\r\n          })\r\n              .then((res) => res.json())\r\n              .then(async (res) => {\r\n                  const resData = await res;\r\n                  console.log(resData);\r\n                  if (resData.status === \"success\") {\r\n                    alert(\"Message Sent\");\r\n                  } else if (resData.status === \"fail\") {\r\n                    alert(\"Message failed to send\");\r\n                  }\r\n              });\r\n      } catch (error) {\r\n          console.log(error.message);\r\n      }\r\n\r\n      setAssignResumeDetails({\r\n \r\n      });\r\n      \r\n     \r\n  }\r\n  \r\n  \r\n    return (\r\n        <div className=\"container  mx-auto w-1/2 py-16\">\r\n        <form className=\"space-y-8 divide-y divide-gray-200\" onSubmit={handleSubmit}>\r\n        <div className=\"space-y-8 divide-